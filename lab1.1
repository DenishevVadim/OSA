#include "pch.h"
#include <iostream>
#include <stack>

using namespace std;
int stackSwap(stack<int>&, stack<int>&,int);

int main()
{
	setlocale(LC_ALL, "rus");
	int N, a;
	stack <int> stack1;
	stack <int> stack2;
	stack <int> stack3;
	cout << "Введите размер стека" << endl;
	cin >> N;
	for (int i = 0; i < N; i++) {
		cout << "Введите " << i + 1 << " элемент стэка" << endl;
		cin >> a;
		stack1.push(a);
	}
	stackSwap(stack1, stack2, N);
	stackSwap(stack2, stack3, N);
	for (int i = 0; i < N; i++) {
		cout << stack3.top() << endl;
		stack3.pop();
	}
}
int stackSwap(stack<int> &stackone, stack<int> &stacktwo,int N) {
	for (int i = 0; i < N; i++) {
		stacktwo.push(stackone.top());
		stackone.pop();
	}
	return 0;
}
